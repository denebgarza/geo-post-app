{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "import/extensions": ["error", "never", {"js": "always"}],
      "import/no-extraneous-dependencies": "off"
    }
}
